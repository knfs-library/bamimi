{
  "name": "bamimi",
  "version": "1.0.0-internal",
  "description": "CMS for nodejs",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "start-dev": "nodemon ./src/server.js",
    "schedule": "node ./src/schedule.js",
    "socket": "node ./src/socket.js",
    "socket-dev": "nodemon ./src/socket.js",
    "build": "npx ncc build src/server.js -o dist --v8-cache -m",
    "build-socket": "npx ncc build src/socket.js -o dist --v8-cache -m",
    "build-schedule": "npx ncc build src/schedule.js -o dist --v8-cache -m",
    "migration:create": "npx sequelize-cli migration:generate",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:knfs-jsc/bamimi.git"
  },
  "author": "Kent Phung",
  "keywords": [
    "cms",
    "framework"
  ],
  "license": "MIT",
  "dependencies": {
    "@knfs-tech/bamimi-auth": "^1.0.0",
    "@knfs-tech/bamimi-autoload": "^1.0.6",
    "@knfs-tech/bamimi-socket.io": "^1.0.5",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "bullmq": "^5.7.1",
    "commander": "^9.0.0",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.6",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.9",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.3.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "ps-node": "^0.1.6",
    "response-time": "^2.3.2",
    "sequelize": "^6.37.3",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.0",
    "winston": "^3.13.0"
  },
  "autoload": {
    "files": {
      "./src/utils/asset.js": "asset",
      "./src/libs/log.js": "logger"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.20",
    "@faker-js/faker": "^7.6.0",
    "@vercel/ncc": "^0.38.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-register": "^6.26.0",
    "eslint": "^8.34.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.4.1",
    "socket.io-client": "^4.6.1",
    "supertest": "^6.3.3",
    "webpack-cli": "^5.1.4"
  },
  "private": true
}
